{
  "name": "@vuepress/bundler-vite",
  "version": "2.0.0-rc.24",
  "description": "Bundler vite package of VuePress",
  "keywords": [
    "vuepress-bundler",
    "vuepress",
    "bundler",
    "vite"
  ],
  "homepage": "https://github.com/vuepress",
  "bugs": {
    "url": "https://github.com/vuepress/core/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuepress/core.git"
  },
  "license": "MIT",
  "author": "meteorlxy",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./client": "./client.d.ts",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "client.d.ts"
  ],
  "dependencies": {
    "@vitejs/plugin-vue": "^6.0.0",
    "autoprefixer": "^10.4.21",
    "connect-history-api-fallback": "^2.0.0",
    "postcss": "^8.5.6",
    "postcss-load-config": "^6.0.1",
    "rollup": "^4.44.1",
    "vite": "~7.0.0",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "@vuepress/bundlerutils": "2.0.0-rc.24",
    "@vuepress/client": "2.0.0-rc.24",
    "@vuepress/core": "2.0.0-rc.24",
    "@vuepress/utils": "2.0.0-rc.24",
    "@vuepress/shared": "2.0.0-rc.24"
  },
  "devDependencies": {
    "@types/connect-history-api-fallback": "^1.5.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "tsup": {
    "clean": true,
    "dts": "./src/index.ts",
    "entry": [
      "./src/index.ts"
    ],
    "format": [
      "esm"
    ],
    "outDir": "./dist",
    "sourcemap": false,
    "target": "es2023",
    "tsconfig": "../../tsconfig.dts.json"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist"
  }
}